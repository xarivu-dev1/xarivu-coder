<UserControl x:Class="Xarivu.CoderTest.View.TesMenuControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Xarivu.CoderTest.View"
             xmlns:coder_view_control="clr-namespace:Xarivu.Coder.View.Control;assembly=Xarivu.Coder"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="200">
    <Grid>
        <ToolBar VerticalAlignment="Stretch" VerticalContentAlignment="Center" FocusManager.IsFocusScope="False">
            <coder_view_control:ToolbarButton
                    Text="Populate"
                    Command="{Binding PopulateCommand}"/>
        </ToolBar>
    </Grid>
</UserControl>
